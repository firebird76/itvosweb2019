@model IEnumerable<Country>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Car code</th>
        <th>Population</th>
      </tr>
    </thead>
    <tbody>
      @foreach (var country in Model)
      {
        <tr>
          <td>@country.Name</td>
          <td>@country.CarCode</td>
          <td>@country.Population</td>
        </tr>
      }
    </tbody>
  </table>