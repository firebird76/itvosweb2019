@model FirmaVM

@{
    ViewData["Title"] = "Seite2";
}

<h1>Seite2</h1>

<h2>@Model.Name</h2>
<table class="table table-striped">
  @foreach (var person in Model.Mitarbeiter)
  {
    <tr>
      <td>@person.Vorname</td>
      <td>@person.Nachname</td>
    </tr>
  }
</table>
<div>
  <a asp-action="index" class="btn btn-info">Index</a>

</div>